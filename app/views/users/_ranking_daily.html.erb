<h5>デイリーランキング</h5>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ランキング</th>
      <th scope="col">ユーザ情報</th>
    </tr>
  </thead>
  <tbody>
    <% @dailyranking.each_with_index do |ranker, i| %>
    <% rankeracc = @client.user(ranker.twid.to_i) %>
    <tr>
      <th scope="row"><%= i + 1 %></th>
      <td><%= render ranker %></td>
    </tr>
    <% end %>
  </tbody>
</table>
